{
  "manifest_version": 3,
  "name": "Twitter Tweet Filter",
  "version": "1.0",
  "description": "Filter tweets based on custom rules",
  "permissions": ["scripting", "storage"],
  "host_permissions": ["https://twitter.com/*"],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["content.js"]
    }
  ]
}
